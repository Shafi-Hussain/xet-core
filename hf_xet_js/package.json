{
	"name": "@huggingface/xet",
	"author": "Assaf Vayner <assaf@huggingface.co>",
	"version": "0.1.0",
	"description": "WebAssembly bindings for xet storage backend",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*.js",
		"dist/**/*.d.ts",
		"pkg/**/*.js",
		"pkg/**/*.d.ts",
		"pkg/**/*.wasm",
		"pkg/**/*.wasm.d.ts"
	],
	"keywords": [
		"huggingface",
		"xet",
		"WebAssembly"
	],
	"license": "MIT",
	"scripts": {
		"lint": "eslint --quiet --fix --ext .cjs,.ts eslint.config.mjs",
		"lint:check": "eslint --ext .cjs,.ts eslint.config.mjs",
		"format": "prettier --write package.json .prettierrc eslint.config.mjs *.md lib/*",
		"format:check": "prettier --check package.json .prettierrc eslint.config.mjs *.md lib/*",
		"build:wasm-pack": "wasm-pack build",
		"build:typescript": "tsc -b --pretty -v",
		"build": "npm run build:wasm-pack && npm run build:typescript",
		"package": "npm pack"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.23.0",
		"@typescript-eslint/eslint-plugin": " ^8.28.0",
		"@typescript-eslint/parser": "^8.28.0",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.5",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"typescript": "5.1.3",
		"wasm-pack": "0.12.0"
	},
	"packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
